{
  "name": "geographic",
  "scripts": {
    "clean": "rimraf dist .parcel-cache",
    "start": "parcel index.html",
    "build": "npm run clean && parcel build index.html --dist-dir dist --no-source-maps"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.0.3",
    "jquery": "^3.5.1",
    "mapbox-gl": "^3.12.0",
    "ol": "6.4.3",
    "ol-mapbox-style": "^6.9.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.15.2",
    "buffer": "^6.0.3",
    "parcel": "^2.12.0",
    "rimraf": "^5.0.10",
    "sass": "^1.89.2"
  },
  "targets": {
    "default": {
      "outputFormat": "esmodule",
      "isLibrary": false,
      "distDir": "./dist"
    }
  },
  "htmlnano": {
    "minifySvg": false
  }
}
